    <nz-divider nzText="Custom" nzOrientation="left"></nz-divider>
    <button *ngFor="let color of customColors" nz-button nz-tooltip [nzTooltipTitle]="color" [nzTooltipColor]="color">
      {{ color }}
    </button>
  