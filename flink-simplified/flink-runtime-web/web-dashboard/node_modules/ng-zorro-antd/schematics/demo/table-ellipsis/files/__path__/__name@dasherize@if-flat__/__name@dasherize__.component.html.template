
    <nz-table #fixedTable [nzData]="listOfData" nzTableLayout="fixed">
      <thead>
        <tr>
          <th>Full Name</th>
          <th>Age</th>
          <th>Column 1</th>
          <th>Column 2</th>
          <th nzEllipsis>Column ColumnColumn 3</th>
          <th>Column 4</th>
        </tr>
      </thead>
      <tbody>
        <tr *ngFor="let data of fixedTable.data">
          <td>{{ data.name }}</td>
          <td>{{ data.age }}</td>
          <td nzEllipsis>{{ data.address }}</td>
          <td nzEllipsis>{{ data.address }}</td>
          <td nzEllipsis>{{ data.address }}</td>
          <td nzEllipsis>{{ data.address }}</td>
        </tr>
      </tbody>
    </nz-table>
  